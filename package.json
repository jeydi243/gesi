{
  "name": "gesi",
  "version": "0.1.0",
  "private": true,
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "es": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "tail": "npx tailwindcss -i ./src/assets/tailwind.css -o ./src/assets/css/style.css --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fullcalendar/core": "^5.10.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@fullcalendar/vue": "^5.10.1",
    "@fullcalendar/vue3": "^5.10.1",
    "@headlessui/vue": "^1.7.7",
    "@heroicons/vue": "^1.0.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vueuse/components": "^8.9.4",
    "@vueuse/core": "^8.9.4",
    "@vueuse/electron": "^8.9.4",
    "@vueuse/integrations": "^8.9.4",
    "animejs": "^3.2.1",
    "apexcharts": "^3.32.1",
    "axios": "^0.24.0",
    "boxicons": "^2.1.2",
    "chance": "^1.1.8",
    "country-flag-icons": "^1.5.5",
    "date-fns": "^2.28.0",
    "epic-spinners": "^1.1.0",
    "gsap": "^3.9.1",
    "node-watch": "^0.7.3",
    "pinia": "^2.0.18",
    "postcss": "^8.4.12",
    "splitting": "^1.0.6",
    "tw-elements": "^1.0.0-alpha12",
    "typed.js": "^2.0.12",
    "v-calendar": "^3.0.0-alpha.8",
    "validator": "^13.7.0",
    "vee-validate": "^4.7.3",
    "vite": "^4.3.5",
    "vue": "^3.2.36",
    "vue-3-socket.io": "^1.0.4",
    "vue-cal": "^4.2.0",
    "vue-chartjs": "^3.5.1",
    "vue-final-modal": "^3.4.3",
    "vue-nprogress": "^0.2.0",
    "vue-router": "^4.0.13",
    "vue-sweetalert2": "^5.0.2",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-apexcharts": "^1.4.1",
    "vue3-star-ratings": "^1.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/devtools": "^6.2.1",
    "autoprefixer": "^10.4.4",
    "electron": "^22.0.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-vite": "^1.0.22",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "node-sass": "^7.0.0",
    "postcss-import": "^14.1.0",
    "sass": "^1.26.5",
    "sass-loader": "^12.4.0",
    "tailwindcss": "^3.0.0",
    "vite": "^4.3.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/valid-v-model": 0,
      "vue/require-component-is": 0,
      "vue/component-definition-name-casing": [
        "off",
        "kebab-case",
        "PascalCase"
      ],
      "vue/singleline-html-element-content-newline": [
        "off",
        {
          "ignoreWhenNoAttributes": true,
          "ignoreWhenEmpty": true,
          "ignores": [
            "pre",
            "textarea"
          ]
        }
      ],
      "vue/html-indent": [
        "off",
        "4",
        {
          "attribute": 2,
          "baseIndent": 2,
          "closeBracket": 0,
          "alignAttributesVertically": true,
          "ignores": []
        }
      ],
      "vue/html-closing-bracket-newline": "off",
      "vue/max-attributes-per-line": [
        "off",
        {
          "singleline": {
            "max": 7
          },
          "multiline": {
            "max": 7
          }
        }
      ],
      "vue/html-self-closing": [
        "off",
        {
          "html": {
            "void": "never",
            "normal": "always",
            "component": "always"
          },
          "svg": "always",
          "math": "always"
        }
      ],
      "vue/attributes-order": [
        "off",
        {
          "order": [
            "DEFINITION",
            "LIST_RENDERING",
            "CONDITIONALS",
            "RENDER_MODIFIERS",
            "GLOBAL",
            [
              "UNIQUE",
              "SLOT"
            ],
            "TWO_WAY_BINDING",
            "OTHER_DIRECTIVES",
            "OTHER_ATTR",
            "EVENTS",
            "CONTENT"
          ],
          "alphabetical": false
        }
      ],
      "vue/script-indent": [
        "off",
        2,
        {
          "baseIndent": 4,
          "switchCase": 1,
          "ignores": []
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
